<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.smsscheduler">
    <uses-permission android:name="android.permission.SEND_SMS"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/custom_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/custom_icon_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.example.smsscheduler.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.smsscheduler.ScheduledSMSListActivity">
        </activity>
        <activity
            android:name="com.example.smsscheduler.ScheduledSMSOverviewActivity"
            android:screenOrientation="portrait">
        </activity>
    </application>

</manifest>